@model List<SURoomieFinder.Models.Responses>
@{
    ViewBag.Title = "View Matches";
}

<h2>ViewMatches</h2>

<table class=" = table">
	<tr>
		<th>
			First Name
		</th>
		<th>
			Match Percentage
		</th>
		<th>
		</th>
		<th>
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@item.myClient.FirstName
			</td>
			<td>
				@item.myMatchScore
			</td>
			<td>
				@Html.ActionLink("Details", "Details", new { id = item.myClient.Id })
			</td>
			<td>
				@Html.ActionLink("View Answers", "ViewAnswers", new { id = item.myClient.Id })
			</td>
		</tr>
	}

</table>